{
   "lock": false,
   "version": "0.5.0",
   "tasks": {
      "static": "cp static/* -R dist/",
      "build": "deno -A build.ts",
      "tail": "tailwindcss -i ./styles.css -o ./dist/styles.css",
      "serve": "deno run -A jsr:@std/http/file-server dist/"
   },
   "imports": {
      "@preact-icons/fa": "jsr:@preact-icons/fa@^1.0.12",
      "@preact/signals": "npm:@preact/signals@^2.0.1",
      "@sholvoir/components/": "https://raw.esm.sh/gh/sholvoir/preact-components@0.0.9/islands/",
      "@sholvoir/generic": "jsr:@sholvoir/generic@^0.2.1",
      "esbuild": "npm:esbuild@^0.25.0",
      "esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.11.1",
      "preact": "npm:preact@^10.25.4"
   },
   "compilerOptions": {
      "jsx": "react-jsx",
      "jsxImportSource": "preact"
   }
}